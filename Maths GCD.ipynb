    "def gcd(a, b):\n",
    "    r = a%b\n",
    "    q = a//b\n",
    "    print(a,\" = \",b,\" * \",q,\" + \",r)\n",
    "    if(r != 0):\n",
    "        gcd(b, r)\n",
    "    else:\n",
    "        print(\"\\n GCD of \",n1,\" and \",n2,\" is \",b)\n",
    "    \n",
    "\n",
    "n1 = int(input(\"Enter the first number : \"))\n",
    "n2 = int(input(\"Enter the second number : \"))\n",
    "gcd(n1, n2)"
